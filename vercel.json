{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true,
      "feature/*": true,
      "release/*": true
    }
  }
}
